{% extends '../baseFront.html.twig' %}

{% block title %}Cour{% endblock %}

{% block body %}
    <h1>Cour</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cour.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ cour.nom }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ cour.image }}</td>
            </tr>
            <tr>
                <th>Fichier</th>
                <td>{{ cour.fichier }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_cour_index') }}">back to list</a>

    <a href="{{ path('app_cour_edit', {'id': cour.id}) }}">edit</a>

    {{ include('cour/_delete_form.html.twig') }}
